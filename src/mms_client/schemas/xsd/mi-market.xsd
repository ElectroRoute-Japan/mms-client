      This is the XML schema for Interface (II) data submitted/accessed by TSO/BSP.

      History:

      001  06-SEP-2019  RN        Initial version.
      002  01-NOV-2019  RN        Updated to FD Rev D.1 and merged the Switching xsd.
      003  22-NOV-2019  DM/RN/AC  Updated to FD Rev E.
      004  24-Dec-2019  ABB       Changes by JPABB as per XSD_PointOut_R08.xlsx and
                                  HowToSolveIssuesForWebAPI.pptx
      005  23-Jan-2020  RN/AC     Updated to FD Rev E.1, minor change to Messages elements
                                  and made file extensions lower case.
      006  22-MAY-2020  RN/DM     Added PASSED_PARTIAL ValidationStatus,
                                  Added SttlDate/FileSize/FileExten restriction for TSO Settl,
                                  Changed InterconEquipment from Integer to String,
                                  Removed Pending Status for Awards, Add TSO Request acknowledgement,
                                  Removed Commodity, Add Request Status and minor fixes in SwitchQuery,
                                  Changed Float to Decimal for TotalContractCost in AwardResults Summary
      007  22-Jul-2020  RN/DM/AC  1421049: Remove Commodity Category from Switching query results to align with FD
                                  1421516: Make Participant Name and Company Short Name optional in TSO Request
                                  query results to align with FD
                                  1417651: Make Remark in BupStartUpCost as Optional to match FD
                                  1430405: Add Commodity Category to Switching data at customer request
                                  CR-26: Updated FileName format for Baselineload File
      008  21-Aug-2020  RN/DM/AC  CR-12: Updated the enumerations to code per CR-12
                                  and updated the JapaneseStringTypes to version v5
      009  17-Mar-2021  RN/DM     Updated with changes for Tertiary(1)
      010  24-Mar-2021  RN        Updated to have NumOfDays range diff for Submit and Query.
      011  25-May-2021  RN/DM     Updated 6 required elements to optional in Award Results and Switch Query
                                  to align XSD with FD. And Fix for SF-01583980 to increase limit for TotalContractCost.
      012  30-Jul-2021  RN        Updated ParticipantNameType to accommodate OMI and other such participants
      013  25-Apr-2022  RN        Updated FromCap Type in BUP to accommodate negative values
      014  22-Nov-2022  DM        Updated for Phase 3 requirements
	  015  03-May-2023  DM        Updated for CR-0010

      </xs:documentation>
    </xs:annotation>

    <!-- BEGIN: Common Simple Type Definitions -->

    <xs:simpleType name="MarketActionType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="SUBMIT"/>
        <xs:enumeration value="QUERY"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ValidationStatusType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="PASSED"/>
        <xs:enumeration value="WARNING"/>
        <xs:enumeration value="PASSED_PARTIAL"/>
        <xs:enumeration value="FAILED"/>
        <xs:enumeration value="NOT_DONE"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CodeType">
      <xs:restriction base="xs:string">
        <xs:maxLength value="50"/>
        <xs:minLength value="2"/>
        <xs:pattern value="[a-zA-Z_0-9\-]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TransactionId">
      <xs:restriction base="xs:string">
        <xs:maxLength value="10" />
        <xs:minLength value="8" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AttachmentActionType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="DELETE"/>
        <xs:enumeration value="NEW"/>
        <xs:enumeration value="NONE"/>
      </xs:restriction>
    </xs:simpleType>

    <!-- END: Common Simple Type Definitions -->

    <!-- BEGIN: Registration Simple Type Definitions -->

    <xs:simpleType name="ParticipantNameType">
      <xs:restriction base="xs:string">
        <xs:length value="4"/>
        <xs:pattern value="[A-Z]([0-9][0-9][1-9]|[0-9][1-9][0-9]|[1-9][0-9][0-9])"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="UserNameType">
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="12"/>
        <xs:pattern value="[A-Z0-9]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ResourceNameType">
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="10"/>
        <xs:pattern value="[A-Z0-9_\-]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CompanyShortNameType">
      <xs:restriction base="JapaneseStringType">
        <xs:minLength value="1"/>
        <xs:maxLength value="10"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ResourceShortNameType">
      <xs:restriction base="JapaneseStringType">
        <xs:minLength value="1"/>
        <xs:maxLength value="10"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SystemCodeType">
      <xs:restriction base="xs:string">
        <xs:length value="5"/>
        <xs:pattern value="[A-Z0-9]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="OfferIdType">
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="30"/>
        <xs:pattern value="[a-zA-Z0-9_-]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ContractSourceType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="1"/>
        <xs:enumeration value="2"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="OperatorCodeType">
      <xs:restriction base="xs:string">
        <xs:length value="4"/>
        <xs:pattern value="[A-Z0-9]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CommandOperationMethodPriSec1Type">
      <xs:restriction base="xs:string">
        <xs:enumeration value="1"/>
        <xs:enumeration value="3"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CommandOperationMethodSec2Ter1Ter2Type">
      <xs:restriction base="xs:string">
        <xs:enumeration value="1"/>
        <xs:enumeration value="2"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ResourceTypeType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="01"/>
        <xs:enumeration value="02"/>
        <xs:enumeration value="03"/>
        <xs:enumeration value="04"/>
        <xs:enumeration value="05"/>
        <xs:enumeration value="06"/>
        <xs:enumeration value="07"/>
      </xs:restriction>
    </xs:simpleType>

    <!-- END: Registration Simple Type Definitions -->

    <!-- BEGIN: General Simple Type Definitions -->

    <xs:simpleType name="FreeStringType">
      <xs:restriction base="xs:string">
        <xs:pattern value="[a-zA-Z0-9 ~!@#$*()_+}{:?`='/.,%;\^\|\-\]\[\\&lt;&gt;&amp;&quot;]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="JapaneseStringType">
      <xs:restriction base="xs:string">
        <xs:pattern value="[&#12288;-&#12543;&#65280;-&#65376;&#65440;-&#65519;&#19968;-&#40938;]*"/>
     </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AsciiJapaneseStringType">
      <xs:restriction base="xs:string">
        <xs:pattern value="[a-zA-Z0-9 ~!@#$*()_+}{:?>`='/.,%;\^\|\-\]\[\\&lt;&amp;&quot;&#12288;-&#12543;&#65280;-&#65376;&#65440;-&#65519;&#19968;-&#40938;]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="RemarkType">
      <xs:restriction base="AsciiJapaneseStringType">
        <xs:minLength value="1"/>
        <xs:maxLength value="30"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PatternRemarkType">
      <xs:restriction base="AsciiJapaneseStringType">
        <xs:minLength value="1"/>
        <xs:maxLength value="50"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DrPatternNameType">
      <xs:restriction base="JapaneseStringType">
        <xs:minLength value="1"/>
        <xs:maxLength value="10"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="FileNameType">
      <xs:restriction base="FreeStringType">
        <xs:minLength value="10"/>
        <xs:maxLength value="60"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SettlementFileNameType">
      <xs:restriction base="FreeStringType">
        <xs:minLength value="18"/>
        <xs:maxLength value="60"/>
        <xs:pattern value="[A-Z0-9]{4}_[A-Z0-9]{4}_[A-Z0-9_-]{4,46}\.(pdf|zip|csv|xml)"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BaselineLoadFileNameType">
      <xs:restriction base="FreeStringType">
        <xs:minLength value="31"/>
        <xs:maxLength value="40"/>
        <xs:pattern value="W9_[0-9]{4}_[0-9]{8}_[0-9]{2}_[A-Z0-9]{5}_[A-Z0-9_\-]{1,10}\.xml"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="InterconEquipmentType">
      <xs:restriction base="xs:string">
        <xs:length value="2"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BandNumberType">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="20"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StackNumberType">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="20"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DrPatternNumberType">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="20"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PatternNumberType">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="10"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BupBandNumberType">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="20"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AbcBandNumberType">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="5"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StartupCostCaseNumberType">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="10"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NumOfDays31Type">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="31"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NumOfDays7Type">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="7"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ContractIdType">
      <xs:restriction base="xs:string">
        <xs:minLength value="19"/>
        <xs:maxLength value="19"/>
        <xs:pattern value="[a-zA-Z0-9]*"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="RankType">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="9999999"/>
      </xs:restriction>
    </xs:simpleType>

    <!-- END: General Simple Type Definitions -->

    <!-- BEGIN: Precision Types -->

    <xs:simpleType name="Id18Type">
      <xs:restriction base="xs:long">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="999999999999999999"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="KwPosValue7.0Type">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="9999999"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="KwPosNegValue7.0Type">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="-9999999"/>
        <xs:maxInclusive value="9999999"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosValue9.0Type">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="999999999"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="KwPosValue13.0Type">
      <xs:restriction base="xs:long">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="9999999999999"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosValue12.2Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.00"/>
        <xs:maxInclusive value="9999999999.99"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosValue14.2Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.00"/>
        <xs:maxInclusive value="999999999999.99"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="MwPosValue7.3Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.000"/>
        <xs:maxInclusive value="9999.999"/>
        <xs:fractionDigits value="3" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="MwNegValue7.3Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="-9999.999"/>
        <xs:maxInclusive value="0"/>
        <xs:fractionDigits value="3" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="MwValue7.3Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="-9999.999"/>
        <xs:maxInclusive value="9999.999"/>
        <xs:fractionDigits value="3" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PriceValue6.2RoundedType">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.00"/>
        <xs:maxInclusive value="10000.00"/>
        <xs:fractionDigits value="2" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PriceValue8.2RoundedType">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.00"/>
        <xs:maxInclusive value="1000000.00"/>
        <xs:fractionDigits value="2" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosValue6.2Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.00"/>
        <xs:maxInclusive value="9999.99"/>
        <xs:fractionDigits value="2" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosNegValue6.2Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="-9999.99"/>
        <xs:maxInclusive value="9999.99"/>
        <xs:fractionDigits value="2" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosValue6.1Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.0"/>
        <xs:maxInclusive value="99999.9"/>
        <xs:fractionDigits value="1" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosNegValue6.1Type">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="-99999.9"/>
        <xs:maxInclusive value="99999.9"/>
        <xs:fractionDigits value="1" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosValue4.2RoundedType">
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.00"/>
        <xs:maxInclusive value="100.00"/>
        <xs:fractionDigits value="2" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosValue3.0Type">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="999"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PosValue8.0Type">
      <xs:restriction base="xs:int">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="99999999"/>
      </xs:restriction>
    </xs:simpleType>

    <!-- END: Precision Types -->

    <!-- BEGIN: JBMS Simple Enumeration Types -->

    <xs:simpleType name="MarketTypeType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="DAM"/>
        <xs:enumeration value="WAM"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CommodityCategoryType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="1000"/>  <!-- Primary              -->
        <xs:enumeration value="2100"/>  <!-- Secondary 1          -->
        <xs:enumeration value="2200"/>  <!-- Secondary 2          -->
        <xs:enumeration value="3100"/>  <!-- Tertiary 1           -->
        <xs:enumeration value="3200"/>  <!-- Tertiary 2           -->
        <xs:enumeration value="4000"/>  <!-- Compound / Whole WAM -->
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DirectionUpDownType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="1" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DirectionFwdBkwdType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="0" />
        <xs:enumeration value="1" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AfterGCType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="0" />
        <xs:enumeration value="1" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ProcurementAreaType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="01"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AwardStatusLimitedType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="ACTIVE"/>
        <xs:enumeration value="SWITCHED"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AwardStatusType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="ACTIVE"/>
        <xs:enumeration value="SWITCHED"/>
        <xs:enumeration value="TENTATIVE"/>
        <xs:enumeration value="TSO"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="RequestStatusType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="Requested"/>
        <xs:enumeration value="Acknowledged"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="OfferAwardedLevelType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="1"/>
        <xs:enumeration value="2"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AreaType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="01"/>
        <xs:enumeration value="02"/>
        <xs:enumeration value="03"/>
        <xs:enumeration value="04"/>
        <xs:enumeration value="05"/>
        <xs:enumeration value="06"/>
        <xs:enumeration value="07"/>
        <xs:enumeration value="08"/>
        <xs:enumeration value="09"/>
        <xs:enumeration value="10"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DestinationTypeType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="TSO" />
        <xs:enumeration value="Tentative" />
        <xs:enumeration value="Resource" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PatternStatusType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="1" />
        <xs:enumeration value="0" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NPFlag">
      <xs:restriction base="xs:string">
        <xs:enumeration value="1" />
        <xs:enumeration value="2" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AwardTypeType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="MA"/>
        <xs:enumeration value="Switching"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SubRequirementTypeType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="PRI"/>
        <xs:enumeration value="SEC1"/>
        <xs:enumeration value="SEC2"/>
        <xs:enumeration value="TER1"/>
        <xs:enumeration value="PRI-SEC1"/>
        <xs:enumeration value="PRI-SEC2"/>
        <xs:enumeration value="PRI-TER1"/>
        <xs:enumeration value="SEC1-SEC2"/>
        <xs:enumeration value="SEC1-TER1"/>
        <xs:enumeration value="SEC2-TER1"/>
        <xs:enumeration value="PRI-SEC1-SEC2"/>
        <xs:enumeration value="PRI-SEC1-TER1"/>
        <xs:enumeration value="PRI-SEC2-TER1"/>
        <xs:enumeration value="SEC1-SEC2-TER1"/>
        <xs:enumeration value="PRI-SEC1-SEC2-TER1"/>
      </xs:restriction>
    </xs:simpleType>

    <!-- END: JBMS Simple Enumeration Types -->

    <!-- BEGIN: Common Attribute Group Definitions -->

    <xs:attributeGroup name="CommonAttributes">
      <xs:attribute name="Validation" type="ValidationStatusType" use="optional" default="NOT_DONE"/>
      <xs:attribute name="Success" type="xs:boolean" use="optional"/>
    </xs:attributeGroup>

    <xs:attributeGroup name="NonStndCommonAttributes">
      <xs:attribute name="Process" type="xs:boolean" use="optional" />
      <xs:attribute name="Valid" type="xs:boolean" use="optional" />
      <xs:attribute name="Success" type="xs:boolean" use="optional" />
      <xs:attribute name="VersionNo" type="xs:float" use="required" fixed="1.0" />
    </xs:attributeGroup>

    <xs:attributeGroup name="HeaderAttributes">
      <xs:attribute name="Date" type="xs:date" use="required" />
      <xs:attribute name="ParticipantName" type="ParticipantNameType" use="required" />
      <xs:attribute name="UserName" type="UserNameType" use="required" />
      <xs:attribute name="MarketType" type="MarketTypeType" use="optional" />
    </xs:attributeGroup>

    <!-- HeaderSubmit and HeaderQuery should be kept in sync -->
    <xs:attributeGroup name="HeaderSubmitAttributes">
      <xs:attribute name="NumOfDays" type="NumOfDays31Type" use="optional" default="1"/>
    </xs:attributeGroup>

    <xs:attributeGroup name="HeaderQueryAttributes">
        <xs:attribute name="NumOfDays" type="NumOfDays7Type" use="optional" default="1" />
    </xs:attributeGroup>

    <!-- END: Common Attribute Group Definitions -->

    <!-- BEGIN: Common Complex Type Definitions -->

    <xs:complexType name="ErrorType">
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="Code" type="CodeType" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="WarningType">
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="Code" type="CodeType" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="InformationType">
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="Code" type="CodeType" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="MessagesType">
